{
  "name": "fullstack-todo-app",
  "version": "1.0.0",
  "description": "This repository contains the client side and server side code for a full-stack todo list application.",
  "main": "index.js",
  "scripts": {
    "docker:start": "cd server && docker-compose up -d",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run start",
    "dev": "npm run docker:start && concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MREstepa/fullstack-todo-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MREstepa/fullstack-todo-app/issues"
  },
  "homepage": "https://github.com/MREstepa/fullstack-todo-app#readme",
  "dependencies": {
    "concurrently": "^5.3.0"
  }
}
